#!/usr/bin/env python

import sys
import os
sys.path.append(os.path.join(os.path.dirname(__file__), os.path.pardir))
import ktbh
from optparse import OptionParser

if __name__ == '__main__':
    parser = OptionParser()

    options, args = parser.parse_args()

    assert len(args) >= 1
    
    if args[0] == "addlp":
	    url = args[1]
	    ktbh.add_landing_page(url)
